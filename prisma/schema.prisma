generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sorteio {
  id        String   @id @default(cuid())
  nome      String
  whatsapp  String
  email String
  role String
  company String
  objetivo String
  interesse String
  createdAt DateTime @default(now())
}
